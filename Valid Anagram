public class Main{
    public boolean isAnagram (String s, String t){
        s=s.toLowerCase();
        t=t.toLowerCase();
        s=s.replace(" ","");
        t=t.replace(" ","");
        int[] counts=new int[26];
        for(int i=0;i<s.length();i++){
            counts[s.charAt(i)-'a']++;
        }
        for(int i=0;i<t.length();i++){
            counts[t.charAt(i)-'a']--;
        }
        for(int count:counts){
            if(count!=0){
                return false;
            }
        }
        return true;
    }
    public static void main(String[] args){
        Main obj=new Main();
        String str1="Surendra";
        String str2="ardsuren";
        boolean str=obj.isAnagram(str1,str2);
        if(str!=false){
            System.out.println("It is an Anagram");
        }
        else{
            System.out.println("It is not an Anagram");
        }
    }
}
